class Person {
    init(fName, lName, hobby) {
        this.fName = fName;
        this.lName = lName;
        this.hobby = hobby;
    }

    greet() {
        print "Hello, my name is " + this.fName + " " + this.lName + ". My favourite hobby is " + this.hobby + ".";
    }
}

var alex = Person("Alex", "Austen", "Tennis");
alex.greet();

